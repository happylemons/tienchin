<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.emilia.tienchin.mapper.SysUserMapper">
    <select id="findUser" resultType="org.emilia.tienchin.pojo.entity.SysUser">
        select *
        from sys_user
        where user_name = #{username}

    </select>
</mapper>