<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.emilia.tienchin.mapper.SysDeptMapper">


    <select id="selectByDeptId" resultType="org.emilia.tienchin.pojo.entity.SysDept">
        select *
        from sys_dept
        where dept_id = #{depId}
    </select>
</mapper>